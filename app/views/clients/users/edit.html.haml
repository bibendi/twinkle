- breadcrumb :client_users, @client

.page-header
  %h1= t '.title', default: [:'helpers.titles.edit', 'Update %{model}'], model: User.model_name.human.titleize

= form_for @client_user, url: client_user_url(@client, @client_user), html: {class: "form form-horizontal channel"} do |f|
  = render "form", f: f
